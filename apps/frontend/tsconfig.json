{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "src/*": ["./src/*"],
      "@assist/types": ["../../packages/types/src/index.ts"],
      "@assist/types/*": ["../../packages/types/src/*"]
    },
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "allowJs": false,
    "noUnusedLocals": true,
    "strictNullChecks": true
  }
}
