{
  "name": "assist-move-assist-backend",
  "version": "1.0.0",
  "description": "Backend API para Assist Move Assist com PostgreSQL puro",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/app.js",
