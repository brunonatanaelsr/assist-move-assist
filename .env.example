# Configurações do Supabase
VITE_SUPABASE_URL=https://seu-projeto.supabase.co
VITE_SUPABASE_ANON_KEY=sua_chave_publica_supabase
VITE_SUPABASE_SERVICE_ROLE_KEY=sua_chave_service_role

# Configuração de API
VITE_API_BASE_URL=http://localhost:3000/api

# Configurações da Aplicação
VITE_APP_NAME="Assist Move Assist"
VITE_APP_VERSION="1.0.0"
VITE_ENVIRONMENT="development"

# Monitoramento (Opcional)
VITE_SENTRY_DSN=https://sua-chave@sentry.io/projeto
VITE_LOGROCKET_APP_ID=seu-app-id

# Analytics (Opcional)
VITE_GA_MEASUREMENT_ID=GA_MEASUREMENT_ID

# APIs Externas (Opcional)
VITE_WHATSAPP_API_TOKEN=seu_token_whatsapp
VITE_OPENAI_API_KEY=sua_chave_openai

# Configurações de Storage
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# URLs de Redirecionamento
VITE_REDIRECT_URL=http://localhost:3000/auth/callback
VITE_SITE_URL=http://localhost:3000

# Backend

# Variáveis de ambiente para desenvolvimento local
NODE_ENV=development

# Configuração do banco de dados PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=movemarias
POSTGRES_USER=postgres
POSTGRES_PASSWORD=sua_senha_aqui

# Configuração JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Configuração do servidor
PORT=3001
HOST=localhost

# Configuração de logs
LOG_LEVEL=debug

# Configuração de CORS
CORS_ORIGIN=http://localhost:5173

# Configuração de rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Configuração do WebSocket
WS_PORT=3002

# Configuração de upload de arquivos
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880

# Configuração de email (opcional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_FROM=noreply@assistmoveassist.com

# URLs do frontend
FRONTEND_URL=http://localhost:5173
FRONTEND_DOMAIN=localhost:5173

# Configuração de segurança
BCRYPT_ROUNDS=12
SESSION_SECRET=your-session-secret-key

# Configuração de desenvolvimento
DEV_ENABLE_HTTPS=false
DEV_SSL_CERT_PATH=
DEV_SSL_KEY_PATH=
